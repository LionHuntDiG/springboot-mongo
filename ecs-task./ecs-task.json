{
    "family": "web-timer",
    "containerDefinitions": [
        {
            "name": "web",
            "image": "nginx",
            "cpu": 99,
            "memory": 100,
            "portMappings": [
                {
                    "containerPort": 80,
                    "hostPort": 80
                }
            ],
            "essential": true,
            "mountPoints": [
                {
                    "sourceVolume": "webdata",
                    "containerPath": "/usr/share/nginx/html",
                    "readOnly": true
                }
            ]
        }
